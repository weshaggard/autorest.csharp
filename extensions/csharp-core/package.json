{
  "name": "csharp-core",
  "version": "3.0.0",
  "description": "The C# autorest code generator.",
  "main": "dist/main.js",
  "scripts": {
    "start": "node --max_old_space_size=5096 ./dist/main.js",
    "debug": "node --max_old_space_size=4096 --inspect-brk ./dist/main.js",
    "test": "npm run build && mocha dist/test",
    "build": "tsc -p .",
    "watch": "tsc -p . --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Azure/autorest.csharp"
  },
  "readme": "https://github.com/Azure/autorest.csharp/readme.md",
  "keywords": [
    "autorest",
    "csharp"
  ],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Azure/autorest.csharp/issues"
  },
  "homepage": "https://github.com/Azure/autorest.csharp/blob/master/README.md",
  "devDependencies": {
    "@types/js-yaml": "3.11.2",
    "@types/mocha": "5.2.5",
    "@types/node": "10.12.19",
    "mocha": "5.2.0",
    "mocha-typescript": "1.1.17",
    "tslint": "^5.11.0",
    "tslint-language-service-ts3": "1.0.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.1.3",
    "tslint-sonarts": "^1.8.0",
    "@types/xmlbuilder": "0.0.34"
  },
  "dependencies": {
    "@microsoft.azure/autorest-extension-base": "^2.0.0",
    "@microsoft.azure/autorest.remodeler": "^2.0.0",
    "@microsoft.azure/autorest.codemodel-v3": "^2.0.0",
    "source-map-support": "0.5.9"
  }
}
